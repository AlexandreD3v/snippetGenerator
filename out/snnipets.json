"            expmonth.addSelectOption({", 
"                value: '06',", 
"                text: 'Jun'", 
"            });", 
"            expmonth.addSelectOption({", 
"                value: '07',", 
"                text: 'Jul'", 
"            });", 
"            expmonth.addSelectOption({", 
"                value: '08',", 
"                text: 'Aug'", 
"            });", 
"            expmonth.addSelectOption({", 
"                value: '09',", 
"                text: 'Sep'", 
"            });", 
"            expmonth.addSelectOption({", 
"                value: '10',", 
"                text: 'Oct'", 
"            });", 
"            expmonth.addSelectOption({", 
"                value: '11',", 
"                text: 'Nov'", 
"            });", 
"            expmonth.addSelectOption({", 
"                value: '12',", 
"                text: 'Dec'", 
"            });", 
"", 
"            var expyear = form.addField({", 
"                id: 'expyear',", 
"                type: serverWidget.FieldType.SELECT,", 
